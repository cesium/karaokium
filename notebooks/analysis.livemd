```elixir
score = fn list ->
  case list do
    [] ->
      0

    _ ->
      pontuation = Enum.map(list, & &1.pontuation)

      (Enum.sum(pontuation) / Enum.count(list))
      |> Decimal.from_float()
      |> Decimal.round(2)
  end
end
